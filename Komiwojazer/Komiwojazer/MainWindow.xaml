<Window x:Class="Komiwojazer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Komiwojazer"
        mc:Ignorable="d"
        Title="Zagadnienie komiwojażera" Height="371" Width="744">
    <StackPanel>
        <Label Content="Zagadnienie komiwojażera" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="35"
               Padding="30"></Label>
        <Button x:Name="startButton" Content="Start" Cursor="Hand" Margin="50" Height="70" Width="150" FontSize="35" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" VerticalAlignment="Center" HorizontalAlignment="Center" Click="startButton_Click"></Button>
        <Button x:Name="helpButton" Content="Pomoc" Cursor="Hand" Width="50" Margin="550 15 0 0"></Button>
    </StackPanel>
</Window>
