<Window x:Class="Komiwojazer.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Komiwojazer"
        mc:Ignorable="d"
        Title="Zagadnienie komiwojażera" Height="371" Width="744"
        WindowStartupLocation="CenterScreen">
    <StackPanel>
        <Label Content="Zagadnienie komiwojażera" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="35"
               Padding="0" Margin="0,30,0,0"></Label>
        <Button x:Name="startButton" Content="Wersja Pełna" Cursor="Hand" Margin="0,40,0,0" Height="70" Width="250" FontSize="35" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" VerticalAlignment="Center" HorizontalAlignment="Center" Click="startButton_Click"></Button>
        <Button x:Name="demoButton" Content="Wersja Demonstracyjna" Cursor="Hand" Margin="0,20,0,0" Height="70" Width="380" FontSize="35" VerticalContentAlignment="Center" HorizontalContentAlignment="Center" VerticalAlignment="Center" HorizontalAlignment="Center" Click="demoButton_Click" ></Button>
        <Button x:Name="helpButton" Content="Pomoc" Cursor="Hand" Width="50" Margin="550 15 0 0"></Button>
    </StackPanel>
</Window>
